t=0:0.01:1;
n=length(t);
f=cos(4*%pi*t-%pi/2)+sin(2*%pi*t);
ff=fft(f);
ff=[ff((n+1)/2:n),ff(1:(n+1)/2)];
g=f.*(modulo(t*100,10)==0);
fg1=fft(g);
fg=fg1;
fg1=[fg1((n+1)/2:n),fg1(1:(n+1)/2)];
fg(5:(n+1-5))=0;
fg2=[fg((n+1)/2:n),fg(1:(n+1)/2)];
scf
plot(f,'r')
scf
plot2d3(g)
plot(g,'b.')
scf;
plot(abs(ff),'g');
scf;
plot(abs(fg1),'r');
scf;
plot(abs(fg2),'b');
scf;
plot(f,'r')
plot(real(ifft(fg))*10)
